{
  "required": [
    "name",
    "type"
  ],
  "properties": {
    "age": {
      "type": "integer"
    },
    "type": {
      "enum": [
        "cat",
        "dog",
        "bird"
      ],
      "type": "string"
    },
    "name": {
      "minLength": 2,
      "type": "string",
      "pattern": "^[a-zA-Z0-9- ]+$"
    },
    "tags": {
      "items": {
        "minLength": 1,
        "type": "string"
      },
      "type": "array"
    }
  }
}
